{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">

		{{ function('wp_body_open') }}

		<header id="site-header" class="header-footer-group" role="banner">
			{% block header %}
				<div class="header-inner section-inner">
					<div class="header-titles-wrapper">

						<div class="header-titles">
							<h1 class="site-title">
								<a href="{{site.url}}">{{site.name}}</a>
							</h1>
							<div class="site-description">{{site.description}}</div>
						</div><!-- .header-titles -->

						<button
							class="toggle nav-toggle mobile-nav-toggle"
							data-toggle-target=".menu-modal"
							data-toggle-body-class="showing-menu-modal"
							aria-expanded="false"
							data-set-focus=".close-nav-toggle"
						>
							<span class="toggle-inner">
								<span class="toggle-icon">
									{{ twentytwenty_the_theme_svg( 'ellipsis' ) }}
								</span>
								<span class="toggle-text">{{ __('Menu', 'twentytwenty' ) }}</span>
							</span>
						</button><!-- .nav-toggle -->

					</div><!-- .header-titles-wrapper -->

					<div class="header-navigation-wrapper">
						{% if function('has_nav_menu', 'primary') or not function('has_nav_menu', 'expanded')  %}
							<nav class="primary-menu-wrapper" aria-label="{{ __('Horizontal', 'twentytwenty') }}" role="navigation">
								{% include "menu.twig" with {'items': menu.get_items} %}
							</nav><!-- .primary-menu-wrapper -->
						{% endif %}

						{% if enable_header_search == true or function('has_nav_menu', 'expanded') %}
							<div class="header-toggles hide-no-js">
								{% if function('has_nav_menu', 'expanded') %}
									<div class="toggle-wrapper nav-toggle-wrapper has-expanded-menu">
										<button
											class="toggle nav-toggle desktop-nav-toggle"
											data-toggle-target=".menu-modal"
											data-toggle-body-class="showing-menu-modal"
											aria-expanded="false"
											data-set-focus=".close-nav-toggle"
										>
											<span class="toggle-inner">
												<span class="toggle-text">{{ __( 'Menu', 'twentytwenty' ) }}</span>
												<span class="toggle-icon">
													{{ twentytwenty_the_theme_svg( 'ellipsis' ) }}
												</span>
											</span>
										</button><!-- .nav-toggle -->
									</div><!-- .nav-toggle-wrapper -->
								</div><!-- .nav-toggle-wrapper -->
								{% endif %}

								{% if enable_header_search == true %}
									<div class="toggle-wrapper search-toggle-wrapper">
										<button
											class="toggle search-toggle desktop-search-toggle"
											data-toggle-target=".search-modal"
											data-toggle-body-class="showing-search-modal"
											data-set-focus=".search-modal .search-field"
											aria-expanded="false"
										>
											<span class="toggle-inner">
												{{ twentytwenty_the_theme_svg( 'search' ) }}
												<span class="toggle-text">{{ __( 'Search', 'twentytwenty' ) }}</span>
											</span>
										</button><!-- .nav-toggle -->
									</div>
								{% endif %}
								</div><!-- .header-toggles -->
						{% endif %}


					</div><!-- .header-navigation-wrapper -->
				</div><!-- .header-inner -->

				{% if enable_header_search == true %}
					{% include "partial/modal-search.twig" %}
				{% endif %}
			{% endblock %}
		</header><!-- #site-header -->

		{% include "partial/modal-menu.twig" %}

		<main id="site-content" role="main">
			{% block content %}
				Sorry, no content
			{% endblock %}
		</main><!-- #site-content -->

		{% include 'partial/footer-menus-widgets.twig' %}

		{% block footer %}
			<footer id="site-footer" role="contentinfo" class="header-footer-group">
				{% include 'footer.twig' %}
			</footer><!-- #site-footer -->
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
